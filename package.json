{
  "name": "starter-react",
  "scripts": {
    "dev": "bunx --bun vite --clearScreen false",
    "build": "rm dist/app.* dist/prop-types/*; bun run ./scripts/build.js; bun run ./scripts/export-prop-types.js"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "prop-types": "^15.8.1",
    "lodash-es": "^4.0.0",
    "hyperscriptify": "github:effulgentsia/hyperscriptify",
    "@radix-ui/themes": "^2.0.0"
  },
  "devDependencies": {
    "vite": "^5.0.0",
    "@vitejs/plugin-react-swc": "^3.4.1",
    "@drupal-jsx/vite-plugin-drupal": "github:drupal-jsx/vite-plugin-drupal",
    "esbuild": "^0.19.0"
  },
  "bun-create": {
    "postinstall": [
      "./scripts/scaffold.js",
      "bun run build"
    ]
  }
}
